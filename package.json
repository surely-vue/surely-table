    "@surely-vue/prettier-config": "*",
    "@surely-vue/typescript-config": "*",
    "@types/node": "^20.19.7",
    "eslint": "^8.56.0",
    "prettier": "^3.3.3",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2"
  },
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20"
  }
}
