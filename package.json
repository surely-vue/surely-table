{
  "name": "@surely-vue",
  "version": "0.0.0",
  "private": true,
  "description": "surely-vue",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages-*/*"
  ],
  "scripts": {
